<patterns>
    <pattern for="LoadingBar">
        <standard type="colorless" charset="utf8">
            <value>$+oa/$+ot$*pat[$+ol$+ob]$?sn$+on$*pnETA: $+oeta</value>
            <const>
                <filler-space>#</filler-space>
                <anti-space> </anti-space>
            </const>
        </standard>
        <standard type="16color" include-higher="true" charset="utf8">
            <value>$+oa/$+ot$*pat[$+ol$+ob]$?sn$+on$*pnETA: $+oeta</value>
            <const>
                <filler-space>#</filler-space>
                <anti-space>-</anti-space>
            </const>
        </standard>
        <standard type="colorless" charset="ascii">
            <value>$+oa/$+ot$*pat[$+ol$+ob]$?sn$+on$*pnETA: $+oeta</value>
            <const>
                <filler-space>#</filler-space>
                <anti-space> </anti-space>
            </const>
        </standard>
        <standard type="16color" include-higher="true" charset="ascii">
            <value>$+oa/$+ot$*pat[$+ol$+ob]$?sn$+on$*pnETA: $+oeta</value>
            <const>
                <filler-space>#</filler-space>
                <anti-space> </anti-space>
            </const>
        </standard>
    </pattern>
</patterns>
